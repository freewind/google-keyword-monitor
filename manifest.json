{
  "manifest_version": 2,

  "name": "Google keywords monitor",
  "description": "monitor and collect searched google keywords",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}